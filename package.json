{
  "name": "melon-fab",
  "version": "1.0.1",
  "description": "Material Design Floating Action Button for React",
  "main": "./lib/index.js",
  "scripts": {
    "start": "NODE_ENV=dev node tools/server.js",
    "build": "rimraf lib && NODE_ENV=production gulp",
    "lint": "fecs src",
    "docs": "esdoc -c esdoc.json",
    "setup": "rimraf asset && NODE_ENV=production webpack --config=tools/webpack.dll.js",
    "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
    "test-ci": "NODE_ENV=test karma start ./test/karma.ci.conf.js",
    "coveralls": "cat ./test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls"
  },
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-external-helpers-insert-require": "^1.0.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "classnames": "^2.2.5",
    "coveralls": "^2.11.12",
    "css-loader": "^0.24.0",
    "enzyme": "^2.6.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs": "^0.8.6",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-babel-external-helpers": "^2.0.2",
    "gulp-sourcemaps": "^1.6.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.4.1",
    "jasmine-expect-jsx": "^1.1.2",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-expect-jsx": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "nib": "^1.1.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-melon/melon-fab.git"
  },
  "peerDependencies": {
    "react": "^0.14.8||^15.4.0"
  },
  "keywords": [
    "react",
    "ui",
    "form"
  ],
  "author": "ludafa@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-melon/melon-fab/issues"
  },
  "homepage": "https://github.com/react-melon/melon-fab#readme",
  "dependencies": {
    "melon-core": ">=0.4.1",
    "shallow-equal": "^1.0.0"
  },
  "babel": {
    "env": {
      "dev": {
        "presets": [
          "es2015",
          "stage-2",
          "react"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "stage-2",
          "react"
        ],
        "plugins": [
          "istanbul"
        ]
      },
      "production": {
        "presets": [
          [
            "es2015",
            {
              "loose": true,
              "modules": "umd"
            }
          ],
          "stage-2",
          "react"
        ],
        "plugins": [
          "transform-es3-property-literals",
          "transform-es3-member-expression-literals"
        ]
      }
    }
  }
}
